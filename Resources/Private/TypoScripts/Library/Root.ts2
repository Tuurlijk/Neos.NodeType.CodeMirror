prototype(TypoFree.Neos.NodeType.CodeMirror:CodeSnippet) < prototype(TYPO3.TypoScript:Template) {
	templatePath = 'resource://TypoFree.Neos.NodeType.CodeMirror/Private/Templates/TypoScriptObjects/CodeSnippet.html'
	language = ${q(node).property('language')}
	theme = ${q(node).property('theme')}
	enableLineNumbers = ${q(node).property('enableLineNumbers') ? 1 : 0}
	firstLineNumber = ${q(node).property('firstLineNumber')}
	highlightLines = ${q(node).property('highlightLines')}
	source = ${q(node).property('source')}
}